{
    "swagger": "2.0",
    "info": {
      "title": "Access API",
      "version": "1.0.0"
    },
    "paths" : {
        "/upload" : { 
            "post" :{
                "description" : "Upload a file",
                "consumes" : ["multipart/form-data"],
                "parameters" : [
                    {
                        "name" : "file",
                        "in" : "formData",
                        "type" : "file",
                        "required" : true
                    }
                ],
                "responses" : {
                    "201" : {
                        "description" : "File uploaded successfully"
                    }
                },
                "400" : {
                    "description" : "Invalid file or no file selected"
                }
            }
        },
        "/ping" : {
            "get" : {
                "description" : "Ping the server",
                "responses" : {
                    "200" : {
                        "description" : "Server is up and running"
                    }
                }
            }
        }
    }

}